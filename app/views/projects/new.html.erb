<h1>Add a New Project</h1>

<%= form_for(@project) do |f| %>
    <label>Title: </label>
    <%= f.text_field :title %><br>

    <label>Quote: </label>
    <%= f.text_area :quote %><br>

    <label>Style: </label>
    <%= f.text_field :style %><br>

    <%= f.submit "Create Project" %>
<% end %>

