<h1>Welcome Back!</h1>

<%= form_for @user, url: login_path, method: :post do |f| %>
    <label>Email Address: </label>
    <%= f.text_field :email %><br>

    <label>Password: </label>
    <%= f.password_field :password %><br>

    <%= f.submit "Log In" %>

<% end %>

<%= link_to "Log in using Facebook", auth_facebook_callback_path %>